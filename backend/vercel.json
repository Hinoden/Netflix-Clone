{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "content-type"
      }
   }
 ]
}

{  
  "rewrites": [
       { "source": "/proxy/:match*",
         "destination": "https://netflix-clone-api-xi.vercel.app/:match*"
       } 
 ]
}